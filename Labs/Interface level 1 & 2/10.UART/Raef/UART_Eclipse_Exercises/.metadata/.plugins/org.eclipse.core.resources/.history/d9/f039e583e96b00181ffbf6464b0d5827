#include <stdio.h>
#include <stdlib.h>

int main(void) {
	//int y;
	unsigned char x[5];
	x[0]=2;
	x[1]=3;
	x[2]=4;
	x[3]=5;
	x[4]=6;

		float total = x[1] + (float)(x[2]/10);//+(x[3]/100)+(x[4]/1000));
		printf("%f",total); /* prints !!!Hello World!!! */
	return EXIT_SUCCESS;
}
